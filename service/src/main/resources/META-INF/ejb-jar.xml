<ejb-jar
	xmlns = "http://java.sun.com/xml/ns/javaee"
	version = "3.1"
	xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation = "http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd">
	<interceptors>
		<interceptor>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor>
	</interceptors>
	
	<assembly-descriptor>
		<interceptor-binding>
			<ejb-name>AreaServiceBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>AreaTypeNamesServiceBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>BookmarkServiceBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>MapConfigServiceBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>MapFishServiceBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>PropertiesBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>ReportingServiceBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>ServiceLayerServiceBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>SpatialEnrichmentServiceBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>SpatialEventServiceBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>SpatialInitializerBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>SpatialRepositoryBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>SpatialServiceBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>UserAreaServiceBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>		
	</assembly-descriptor>
</ejb-jar>