<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">

    <filter>
        <filter-name>AuthenticationFilterMock</filter-name>
        <filter-class>eu.europe.ec.fisheries.uvms.spatial.rest.AuthenticationFilterMock</filter-class>
        <async-supported>true</async-supported>
    </filter>
    <filter-mapping>
        <filter-name>AuthenticationFilterMock</filter-name>
        <url-pattern>/rest/*</url-pattern>
    </filter-mapping>

    <context-param>
        <param-name>usmApplication</param-name>
        <param-value>Spatial</param-value>
    </context-param>
</web-app>