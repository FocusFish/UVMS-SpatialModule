<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

	<changeSet author="martinhu" id="1462808134896-9">
        <createTable tableName="fmz">
            <column autoIncrement="true" name="gid" type="SERIAL">
                <constraints primaryKey="true" primaryKeyName="fmz_pkey"/>
            </column>
            <column name="geom" type="GEOMETRY(Multipolygon, 4326)"/>
            <column name="fmz_id" type="VARCHAR(8)"/>
            <column name="name" type="VARCHAR(254)"/>
            <column name="code" type="VARCHAR(254)"/>
            <column name="edited" type="date"/>
            <column name="enabled" type="BPCHAR(1)">
                <constraints nullable="false"/>
            </column>
            <column name="enabled_on" type="TIMESTAMP WITHOUT TIME ZONE"/>
        </createTable>
		<sql>create index fmz_geom_index on spatial.fmz using gist(geom)</sql>
    </changeSet>

</databaseChangeLog>
