<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

	<changeSet author="martinhu" id="1462540272744-9">
        <createTable tableName="gfcm">
            <column autoIncrement="true" name="gid" remarks="ID" type="SERIAL">
                <constraints primaryKey="true" primaryKeyName="gfcm_pkey"/>
            </column>
            <column name="geom" type="GEOMETRY(Multipolygon, 4326)"/>
            <column name="name" type="VARCHAR(255)"/>
            <column name="code" type="VARCHAR(20)"/>
            <column name="enabled" type="BPCHAR(1)">
                <constraints nullable="false"/>
            </column>
            <column name="enabled_on" type="TIMESTAMP WITHOUT TIME ZONE"/>
        </createTable>
		<sql>create index gfcm_geom_index on spatial.gfcm using gist(geom)</sql>
    </changeSet>

</databaseChangeLog>
